<body>
<div class="ch-container">
    <div class="box col-md-8" style="align: center;">
        <div class="box-inner">

            <form action="/signup" method="post">
            <div class="box-header well">
                <h2><i class="glyphicon glyphicon-file"></i>Sign Up</h2>
                <div class="box-icon">
                    <button class="btn btn-round btn-primary btn-xs" type="submit" value="Sign up">
                    <i class="glyphicon glyphicon-ok"></i> Submit </button >
                </div>
                </div>
                <div class="box-content">
                    <% for(var i in messages) { %>
                        <div class="flash"><%= messages[i] %></div>
                    <% } %>



                        <h4>First Name</h4>

                        <div class="input-group col-md-12" >
                            <span class="input-group-addon"><i class="glyphicon glyphicon-user red"></i></span>
                            <input type="text" class="form-control" placeholder="First Name" name="firstName" >
                        </div>

                        <h4>Last Name</h4>
                        <div class="input-group col-md-12">
                            <span class="input-group-addon"><i class="glyphicon glyphicon-user red"></i></span>
                            <input type="text" class="form-control" placeholder="Last Name" name="lastName" >
                        </div>


                        <h4>Email</h4>

                        <div class="input-group col-md-12">
                            <span class="input-group-addon"><i class="glyphicon glyphicon-user red"></i></span>
                            <input type="text" class="form-control" placeholder="Email" name="email" >
                        </div>

                        <h4>Username</h4>
                        <div class="input-group col-md-12">
                            <span class="input-group-addon"><i class="glyphicon glyphicon-user red"></i></span>
                            <input type="text" class="form-control" placeholder="Username" name="username" >
                        </div>

                        <h4>Password</h4>
                        <div class="input-group col-md-12">
                            <span class="input-group-addon"><i class="glyphicon glyphicon-user red"></i></span>
                            <input type="password" class="form-control" placeholder="Password" name="password" >
                        </div>

                </div>
            </form>
         </div>
    </div>
    <div class="box col-md-4">
        <div class="box-header well">
